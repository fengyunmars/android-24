<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="0.0dip" />
            <gradient android:startColor="#fff0dfab" android:endColor="#fff0dfab" android:angle="180.0" android:centerY="0.5" />
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <corners android:radius="25.0dip" />
                <gradient android:startColor="#fff59400" android:endColor="#fff59400" android:angle="360.0" android:centerY="0.5" />
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="0.0dip" />
                <gradient android:startColor="#fff59604" android:endColor="#fff59604" android:angle="270.0" android:centerY="0.75" />
            </shape>
        </clip>
    </item>
</layer-list>